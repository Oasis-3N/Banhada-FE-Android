<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.oasis_hackathon_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:networkSecurityConfig="@xml/network_security_config">
        <activity android:name=".endpoints.ArticleInfoView" />
        <activity android:name=".endpoints.CategoryView" />
        <activity android:name=".endpoints.HomeView" />
        <activity android:name=".endpoints.LoginView" />
        <activity android:name=".endpoints.MessageGroupView" />
        <activity android:name=".endpoints.MessageView" />
        <activity android:name=".endpoints.PostArticleView" />
        <activity android:name=".endpoints.ProfileView" />
        <activity android:name=".endpoints.RegisterView" />
        <activity android:name=".endpoints.DeliveryView" />
        <activity android:name=".endpoints.FreshView" />
        <activity android:name=".endpoints.TitleView">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>

</manifest>